<?xml version="1.0" encoding="utf-8"?>
<identity xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    name="SystemInformation"
    namespace="Device"
    owner="$(OEMNAME)"
    legacyName="$(OEMNAME).Device.SystemInformation" xmlns="urn:Microsoft.CompPlat/ManifestSchema.v1.00">
    <onecorePackageInfo
        targetPartition="MainOS"
        releaseType="Production"
        ownerType="OEM" />
    <regKeys>
        <regKey
            keyName="$(hklm.control)\SystemInformation">
            <regValue
                name="SystemProductName"
                type="REG_SZ"
                value="$(PROD)" />
            <regValue
                name="SystemManufacturer"
                type="REG_SZ"
                value="$(OEMNAME)" />
        </regKey>
    </regKeys>
</identity>